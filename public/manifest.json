{
    "manifest_version": 3,
    "name": "URL参数分析器",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/icon16.jpg",
            "32": "icons/icon32.jpg",
            "48": "icons/icon48.jpg",
            "128": "icons/icon128.jpg"
        }
    },
    "icons": {
        "16": "icons/icon16.jpg",
        "32": "icons/icon32.jpg",
        "48": "icons/icon48.jpg",
        "128": "icons/icon128.jpg"
    }
}